
@article{__1989,
	title = {问卷调查数据处理程序设计},
	issn = {1002-6487},
	url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CJFD&dbname=CJFD8589&filename=TJJC198901021&v=},
	abstract = {{\textless}正{\textgreater} 参量控制方法问卷调查根据计算机处理,即是和用计算机将量化后的问卷调查资料进行分组、统计汇总、百分比计算等数据加工处理。在处理过程中,计算机作用的对象是调查中获得的变量。由于实际问卷调查中通常使用了不同的观测指标和提问方法,因而调查获得的变理就呈现出不同的类型。各种类型变量的特征、属性和功能所允许的统计处理方法是各不相同的。这就给处理程序的设计带来难点。但如将一般问卷调查中获得的变量进行合理分类,并引入描述变量特征和属性的参量,便可通过参量来控制程序对各种变量作相应处理。我们称这种方法为参量控制方法。},
	language = {zh-CN},
	number = {1},
	journal = {统计与决策},
	author = {朱, 宁波},
	year = {1989},
	note = {2 citations(CNKI)[2023-8-14]},
	keywords = {存贮结构, 问卷调查数据},
	pages = {35--36},
	file = {问卷调查数据处理程序设计_朱宁波.pdf:C\:\\Users\\yuhangmeng\\Zotero\\storage\\29HV84RM\\问卷调查数据处理程序设计_朱宁波.pdf:application/pdf},
}

